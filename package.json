{
  "name": "auria-server",
  "version": "1.0.0",
  "description": "Auria Server, Node style",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --experimental-modules dist/server.js",
    "migrate:latest": "knex --knexfile src/kernel/database/knexconfig.ts --esm migrate:latest",
    "migrate:rollback": "knex --knexfile src/kernel/database/knexconfig.ts --esm migrate:rollback --all"
  },
  "nodemonConfig": {
    "delay": "1000"
  },
  "keywords": [
    "Auria",
    "Core",
    "NodeJs",
    "Server"
  ],
  "author": "Nicholas Frai",
  "license": "MIT",
  "dependencies": {
    "aurialib2": "file:../auria-lib",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cryptr": "^6.0.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.15",
    "mysql": "^2.18.1",
    "mysql2": "^1.6.5",
    "nanoid": "^3.1.9",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/event-stream": "^3.3.34",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mysql": "^2.15.10",
    "esm": "^3.2.25",
    "esm-ts-node": "^1.0.5",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
